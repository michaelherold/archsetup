! Ubuntu 12.04 requires this to be .Xdefaults as it does not support
! the newers .Xresources. Ideally, this should be .Xresources.

! => Basic Configuration {{{
! =====================================================================
URxvt.cursorBlink:      true
URxvt.depth:            32
URxvt.fading:           15%
URxvt.saveLines:        5000
URxvt.urlLauncher:      google-chrome

URxvt.borderLess:       false
URxvt.borderColor:      #272822
URxvt.internalBorder:   3
URxvt.externalBorder:   3
URxvt.scrollBar:        false
 
!URxvt.transparent:  true
!URxvt.tintColor:    #272822
!URxvt.shading:      90
!URxvt.blurRadius:   5

! }}}
! => Font configuration {{{
! =====================================================================
Xft.antialias:  true
Xft.autohint:   false
Xft.dpi:        96
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

URxvt*font:     xft:Droid Sans Mono:pixelsize=12
URxvt*boldFont: xft:Droid Sans Mono:bold:pixelsize=12

URxvt.letterSpace: -1

! }}}
! => Color Configuration {{{
! =====================================================================
URxvt*background:   #272822
URxvt*foreground:   #f8f8f2
URxvt*cursorColor:  #f8f8f2
URxvt*colorUL:      #31658C

! Tango color scheme
URxvt*color0:       #2e3436
URxvt*color1:       #cc0000
URxvt*color2:       #4e9a06
URxvt*color3:       #edd400
URxvt*color4:       #3465a4
URxvt*color5:       #92659a
URxvt*color6:       #07c7ca
URxvt*color7:       #d3d7cf
URxvt*color8:       #6e706b
URxvt*color9:       #ef2929
